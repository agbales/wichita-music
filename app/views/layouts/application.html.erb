<!DOCTYPE html>
<html>
  <head>
    <title>WichitaMusic</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="row nav-bar">
          <%= link_to "things go by", albums_path %>
          <%= button_to "Sign Up", new_user_registration_path, method: :get, :class => 'button-primary' %>
          <%= button_to "Log In", new_user_session_path, method: :get, :class => 'button-primary' %>
          <%= button_to "Log Out", destroy_user_session_path, method: :delete, :class => 'button-primary' %>
      </div>
      <div>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
